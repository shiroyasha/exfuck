defmodule Exfuck do

  @initial_state %{ :tape => [0], :pointer => 0, :output => "" }

  def run(code) do
    result_state = Enum.reduce code, @initial_state, fn(instruction, state) ->
      run_instruction(instruction, state)
    end

    result[:output]
  end

  defp run_instruction(".", state) do
    IO.puts "here"
  end

end
